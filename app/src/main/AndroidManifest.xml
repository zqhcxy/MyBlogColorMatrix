<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zqh.myblogcolormatrix">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleMatrixActivity"
            android:configChanges="orientation|keyboardHidden|locale|screenSize"/>
        <activity android:name=".SimpleMaxtrixActivity2"
            android:configChanges="orientation|keyboardHidden|locale|screenSize"/>
        <activity android:name=".MyColorMatrixActivity"
            android:configChanges="orientation|keyboardHidden|locale|screenSize"></activity>
    </application>

</manifest>